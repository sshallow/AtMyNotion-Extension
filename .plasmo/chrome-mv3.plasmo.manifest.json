{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "doit_2017@outlook.com",
  "name": "At my notion",
  "description": "Display related notion page when searching!",
  "background": {
    "service_worker": "./static/background/index.ts"
  },
  "options_ui": {
    "page": "./options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.bing.com/*"
      ],
      "js": [
        "static/contents/at-my-notion.tsx"
      ]
    },
    {
      "matches": [
        "*://*.so.com/*"
      ],
      "js": [
        "static/contents/content-360-side.tsx"
      ]
    },
    {
      "matches": [
        "*://*.baidu.com/*"
      ],
      "js": [
        "static/contents/content-baidu-side.tsx"
      ]
    },
    {
      "matches": [
        "*://*.bing.com/*"
      ],
      "js": [
        "static/contents/content.tsx"
      ]
    },
    {
      "matches": [
        "*://*.google.com/*"
      ],
      "js": [
        "static/contents/content-google-side.tsx"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "commands": {
    "test": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "Open the extension main popup"
    }
  }
}
